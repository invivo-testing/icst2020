<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff15743c" android:endColor="#ff1b5934" android:angle="270.0" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <gradient android:startColor="#ff777777" android:endColor="#ff171717" android:angle="270.0" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff7fb883" android:endColor="#ff1b5934" android:angle="270.0" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>