<?xml version="1.0" encoding="utf-8"?>
<selector android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/deleteButton" />
            <corners android:radius="3.0dip" />
            <stroke android:width="5.0px" android:color="@color/deleteRed" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/deleteButtonDark" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
</selector>